# Starknet Whale Tracker Environment Variables
# Copy this to .env and fill in your values

# Required: Starknet RPC (Lava is free)
STARKNET_RPC_URL=https://rpc.starknet.lava.build:443

# Optional: Telegram Alerts
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# Optional: AVNU API (for full DeFi features)
# Get key at https://portal.avnu.fi
AVNU_API_KEY=

# Optional: Other settings
LOG_LEVEL=INFO
